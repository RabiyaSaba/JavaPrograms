// program to check if values are equal 
public class ValuesAreEqual {
	
	public static void main(String args[])
	{
		// two string with same value 
		String s1 = "Welcome";
		String s2 ="Welcome";
		
		// in order to check for values 
		if(s1.equals(s2))
		{
			System.out.println("Values are equal ");
		}
		else
		{
			System.out.println("Values are not equal");
		}
	}

}

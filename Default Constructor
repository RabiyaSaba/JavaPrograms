package constructor;

public class DefaultConstructor {
	// here we have created an object i 
	int i ;
	
	// this is a constructor 
	// default
	// constructor has the same name as class name 
   private  DefaultConstructor()
	{
		System.out.println("We are in Default Constructor :");
		 i =8;
	}
	
	// main 
	public static void main(String args[])
	{
		// make object of constructor 
		
		DefaultConstructor d1 = new DefaultConstructor();
		System.out.println("Constructor called :");
		System.out.println("the value of object is :"+d1.i);
		
		
	}

}

// wap to find the duplicate values in two array
public class FindDuplicateInTwoArray {
	
	public static void main(String args[])
	{
// array are
		int [] a = {1,3,5,6};
		int [] b = {1,4,5,8};
		
		System.out.println("The Duplicate Value is :");
	for (int i =0;i<a.length;i++)
	{
		for (int j =0;j<b.length;j++)
		{
//if a[i] is equal to b[j] then the values are duplicate and print the values 
			if(a[i]==b[j])
			{
				System.out.println(" "+a[i]);
			}
		}
	}
		
		
	}

}


public class G {
	
	public static void main(String args[])
	{
		for(int i=0 ;i<10;i++)
		{
			
			for(int j =0;j<10;j++)
			{
				if(i==0||j==0||i==9&&j<=5||j==6&&i>=5||i==5&&j>=5&&j<=9||j==9&&i>5)
				
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
				
			}
			System.out.println(" ");
		}
	}

}

// wap to find the last occurence of an element 
public class LastOccurence {

	public static void main(String args[])
	{
		// array
		int [] a = {1,3,2,1};
		// element to be checked 
		int target=1;
		// varible to store the value 
		int dummy =0;
		
		for (int i =0;i<a.length;i++)
		{
			// if same then let it execute it to last 
			if(a[i]==target)
			{
			 dummy = i;
			}
		}
		System.out.println("The last occurence of given number is "+dummy);
	}
}


public class HollowLeftAngled {

	
	public static void main(String args[])
	{
		for(int i =0;i<10;i++)
		{
			for(int j=0;j<10;j++)
			{
				// if 
				if(i==0||j==9||i==j)
				{
					System.out.print(" * ");
				}
				else
				{
					System.out.print("   ");
				}
			}
			// next line 
			System.out.println(" ");
		}
	}
}

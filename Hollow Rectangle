
public class HollowRectangle {
	
	public static void main(String args[])
	{
		for(int i =0;i<5;i++)
		{
			// inner loop
			for(int j =0;j<10;j++)
			{
				// cond
				if(j==0|j==9||i==0||i==4)
				{
					// print star
					System.out.print(" * ");
				}
				else
				{
					//print space
					System.out.print("   ");
				}
				
			}
			// next line 
			System.out.println(' ');
		}
	}

}

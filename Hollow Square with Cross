
public class HollowBoxWithCross {
	
	public static void main(String args[])
	{
		for(int i =0;i<10;i++)
		{
			for(int j =0;j<10;j++)
			{
				if(i==0||j==0||i==9||j==9||i==4||j==5)
				{
					// print star
					System.out.print("  * ");
				}
				else
				{
					// print space
					System.out.print("    ");
				}
			}
			// next line 
			System.out.println(" ");
		}
	}

}

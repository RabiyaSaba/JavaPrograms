// program to check whether the given array are same 
import java.util.*;
public class IdentityArray {
	public static void main(String args[])
	{
	
		// Scanner
		Scanner scan = new Scanner(System.in);
	
	// taking 2 array 
	int [ ]a = new int [3];
	int [ ]b = new int [3];
	
	// now lets take values in both 
	System.out.println("Enter the values of first array :");
	for(int i =0;i<3;i++)
	{
		a[i]=scan.nextInt();
	}
	
	// taking second array 
	System.out.println("enter the values of second array :");
	for(int i =0;i<3;i++)
	{
		b[i]=scan.nextInt();
	}

	
	// now checking if identical 
	
	// variable if greater then zero then not identical 
	
	int n =0;
	
	for(int i =0;i<3;i++)
	{
		for(int  j =0;j<3;j++)
		{
			if(a[i]==a[j])
			{
				continue;
			}
			else 
			{
				n++;
			}
		}
	}
	// now check the obtain value 
	if(n>0)
	{
		System.out.println("Not Identical :");
	}
	else
	{
		System.out.println("Identical ");
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
}


public class K{
	
	public static void main(String args[])
	{
		for(int i =0;i<10;i++)
		{
			for(int j=0;j<10;j++)
			{
				if(j==0||i+j==6&&j<=5||i-j==5)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}//esle
			}//inner
			System.out.println(" ");
		}
	}//main

}

// wap to print larger of 3 number 
public class LargerOfThreeNumber {
	
	public static void main(String args[])
	{
		int a =10;
		int b =20;
		int c =30;
		if(a>b && a > c )
		{
			System.out.println("A is greatest");
		}
		if(b>a && b>c)
		{
			System.out.println("B is greatest");
		}
		if(c>a && c>b)
		{
			System.out.println("C is Greatest");
		}
	}

}

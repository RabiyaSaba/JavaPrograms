
public class MagicalBox {
	
	public static void main(String arhs[])
	{
		// outer loop
		for (int i =0;i<10;i++)
		{
			// inner loop
			for(int j=0;j<10;j++)
			{
				// condition to print star
				if(i==0||i==10-1||j==0||j==9||i==j||i+j==10-1||j==5||i==5||i+j==10/2||j-i==10/2)
				{
					System.out.print(" *");
				}
				else
				{
					// to print space
					System.out.print("  ");
				}
			}
			// moving cursor to next line
			System.out.println(" ");
		}
	}

}

package constructor;

public class PublicDefaultConstructor {
	
	// first create an object 
	String name ;
	// create a public constructor
	public PublicDefaultConstructor()
	{
		// now we will assign value to name 
		System.out.println("We are in public default constructor :");
		name ="TAYLOR SWIFT";
	}


// calling main
public static void main(String args[])
{
	// create object of constructor
	PublicDefaultConstructor pd = new PublicDefaultConstructor();
	System.out.println("We are in main :");
	// now print the value of name 
	//in order to do that we will call the value with the help of object created 
	System.out.println(" The name is :"+pd.name );
	
}
}

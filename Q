
public class Q {
	public static void main(String args[])
	{
		for(int i =0;i<10;i++)
		{
			for(int j=0;j<10;j++)
			{
				if(i==0&&j<6||j==6||i==9||j==0||i-j==2&&j>5)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			// next line 
			System.out.println(" ");
		}
	}

}

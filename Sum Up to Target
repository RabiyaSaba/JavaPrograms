// wap to find the element which sum up to target value 
public class SumUpToTarget {
	
	public static void main(String args[])
	{
		int[] a = {1,2,3,4};
		int target = 5;
		int dummy =a[0];
		for (int i =0;i<a.length;i++)
		{
			for(int j =i+1;j<a.length;j++)
			{
				if(a[i]+a[j]==target)
				{
					System.out.println(+a[i]+" + "+a[j]+" = "+target  );
				}
			}
		}
	}

}

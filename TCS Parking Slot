
// TCS question asked 
// basically there is a parking lot 
// if the parking is full 
// we will consider 1 as full 
// and 0 as empty 
// so count the number of cars present

import java.util.*;
public class TCSParkingLot {
	
	
	public static void main(String args[])
	{
		Scanner scan = new Scanner(System.in);
		// now we will create an array 2d 
		int [][] a= new int [3][3];
		// now we will fill the block 
		// now take array from user 
		System.out.println("Enter the parking :");
		for(int i =0;i<3;i++)
		{
			for(int j =0;j<3;j++)
			{
				a[i][j]=scan.nextInt();
				 
			}
		}
		// now we need to find the 1 count 
		// first we will find the length 
		int len = a.length;
		
		int target = 1;
		// filled count
		int fcount=0;
		// filled empty coumt
		int ecount =0;
		// now for loop 
		for(int i =0;i<3;i++)
		{
			for(int j =0;j<3;j++)
			{
				if(target ==a[i][j])
				{
					fcount++;
				}
				else
				{
					ecount++;
				}
				 
			}
		}
			// Display 
			
			System.out.println(" Filled slots :"+fcount);
			System.out.println(" Empty slots :"+ecount);
		

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
	
	
	

}

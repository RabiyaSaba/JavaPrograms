// wap to demostrate the use case of relational operator 
public class UseCaseOfRelationalOperator {
	
	
	public static void main(String args[])
	{
		int a = 10;
		int b =20;
		
		// greater then 
		if(a>b)
		{
			System.out.println("  a is greater value "+a);
		}
		// less 
		if(a<b)
		{
			System.out.println(" a is less then b ");
		}
		if(a>=b)
		{
			System.out.println(" a is greater then equal to be ");
		}
		if(a<=b)
		{
			System.out.println("A is less then equal to be ");
		}
		if(a==b)
		{
			System.out.println(" A is equal to b ");
			
			
			
			
		}
	}

}
